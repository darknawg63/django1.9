{% extends 'base.html' %}
{% load staticfiles %}


{% block branding %}<h1>Musician Administration</h1>{% endblock %}
{% block content %}
<div id="content" class="colM">
  <h1>Add Artist</h1>
<div id="content-main">
  <form action="." class="ajax" method="post">
    {% csrf_token %}
    <div>
      <fieldset class="module aligned ">
        <h2>Artists</h2>
        <div class="form-row">
           <div>
            {{ form.artist_name.label_tag }}
            {{ form.artist_name }}
            {{ form.artist_name.errors }}
          </div>
        </div>
      </fieldset>
    </div>
    <input type="submit" value="Submit" />
  </form>
</div>
</div>
{% endblock %}
{% block javascript %}
  <script src="{% static 'js/jquery.js' %}"></script>
{% endblock %}

